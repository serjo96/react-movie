{
  "extends": ["standard", "standard-react"],
  "rules": {
    "semi": ["error", "always"],
    "array-bracket-spacing":  "error",
    "space-in-parens": "error",
    "space-before-blocks": "error",
    "react/prop-types": ["warn"],
    "react/jsx-fragments": ["warn", "element"],
    "import/no-unresolved": [
     "error",
      {
        "caseSensitive": true
      }]
  },
  "plugins": ["react"],
  "parser": "babel-eslint"
}
