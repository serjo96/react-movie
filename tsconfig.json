{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "ui/*": ["./src/ui-components"],
      "@templates/*": ["./src/Templates"],
      "store/*": ["./src/store/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
